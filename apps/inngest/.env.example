# Upstash Redis
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-rest-token

# Jira session credentials (for Clockwork JWT exchange)
# Obtain by copying the full Cookie header from a Jira browser session
JIRA_FULL_COOKIE=your-full-jira-cookie-header-value
JIRA_DOMAIN=your-org.atlassian.net

# Inngest credentials (from https://app.inngest.com)
INNGEST_EVENT_KEY=your-inngest-event-key
INNGEST_SIGNING_KEY=your-inngest-signing-key
